{
    "name": "frontend-project",
    "version": "0.0.1",
    "appPath": "src/main/webapp/resources",
    "directory": "resources/bower_components",
    "dependencies": {
        "jquery": null,
        "angular": null,
        "angular-messages": null,
        "angular-resource": null,
        "angular-route": null,
        "angular-sanitize": null,
        "bootstrap": null,
        "angular-froala": null,
        "angular-bootstrap": null,
        "ng-bootstrap-submenu": null,
        "bootstrap-submenu": null,
        "codemirror": null
    },
    "resolutions": {
        "angular": "1.3.10"
    },
    
    "exportsOverride": {
    "bootstrap": {
      "js": "dist/js/*.js",
      "css": "dist/css/*.css",
      "fonts": "dist/fonts/*.*"
    },
      "angular-froala": {
      "js": "src/*.js"
    },
      "font-awesome": {
      "css": "css/*.css"
    },
     "codemirror": {
      "css": "lib/*.css",
      "js": "lib/*.js"
    },
     "ng-bootstrap-submenu": {
      "css": "dist/*.css",
      "js": "dist/*.js"
    },
    "bootstrap-submenu": {
      "css": "dist/css/*.css",
      "js": "dist/js/*.js"
    }
    
  }
}